<div class="requests-page-wrapper">
  <div>
    <h1>Request you made: </h1>
    <ul>
      <% @requests_made.each do |request| %>
        <li><%= link_to request.id, request_path(request.id) %></li>
      <% end %>
    </ul>
  </div>
  
  <div>
    <h1>Requests you received: </h1>
    <ul>
      <% @requests_received.each do |request| %>
        <li><%= link_to request.id, request_path(request.id) %></li>
      <% end %>
    </ul>
  </div>
</div>
